def main() {
    struct point {x, y, z}
    struct point points[3]
    points[0].x = 1
    points[0].y = 4
    points[0].z = 6

    points[1].x = 7
    points[1].y = 5
    points[1].z = 3

    i = 2

    points[i].x = 10
    points[i].y = 12
    points[i].z = 13


    index = 0
    while ( index < 3 ) {
        print(points[index].x)
        print(points[index].y)
        print(points[index].z)
        index = index + 1
    }

    struct point my_point
    my_point = &points[1]

    print(~my_point.x)
    print(~my_point.y)
    print(~my_point.z)

} EOP